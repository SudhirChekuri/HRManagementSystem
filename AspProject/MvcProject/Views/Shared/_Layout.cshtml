<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <img alt="logo", src="~/Images/Logo.png", height="100" width="120" />
    
             @* In below actionlink Home-content,Index-Action method name,Home-Controller name *@
    <table>
        <tr><td>@Html.ActionLink("Home", "Index", "Home") |
                @Html.ActionLink("Login", "Login", "Home") | 
                @Html.ActionLink("About Us", "AboutUs", "Home") |  
                @Html.ActionLink("Contact Us", "ContactUs", "Home")  </td></tr>
        <tr><td></td></tr>
    </table>
    
    @RenderBody()
    @Html.ActionLink("UserHome", "Index", "UserHome") <br />
    @Html.ActionLink("WebAdmin", "Index", "WebAdmin") <br />
    <a href="~/HR">HR Home</a><br />
    <a href="~/Manager">Manager Home</a>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
