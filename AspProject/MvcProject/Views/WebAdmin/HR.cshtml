@model IEnumerable<MvcProject.Tbl_HR>

@{
    ViewBag.Title = "HR";
    Layout = "~/Views/Shared/_WebAdmin.cshtml";
}

<h2>HR</h2>

<p>
    @Html.ActionLink("Create New", "HR",new {operation="create" })
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "HR", new { operation="Edit", id=item.Id }) |
            @Html.ActionLink("Details", "HR", new { operation="Details", id=item.Id }) |
            @Html.ActionLink("Delete", "HR", new { operation="Delete", id=item.Id })
        </td>
    </tr>
}

</table>
